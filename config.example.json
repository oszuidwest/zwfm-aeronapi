{
  "database": {
    "host": "localhost",
    "port": "5432",
    "name": "aeron",
    "user": "aeron",
    "password": "",
    "schema": "aeron",
    "sslmode": "disable",
    "max_open_conns": 25,
    "max_idle_conns": 5,
    "conn_max_lifetime_minutes": 5
  },
  "image": {
    "target_width": 640,
    "target_height": 640,
    "quality": 85,
    "reject_smaller": false,
    "max_image_download_size_bytes": 52428800
  },
  "api": {
    "enabled": false,
    "keys": [],
    "request_timeout_seconds": 30
  },
  "maintenance": {
    "bloat_threshold": 10.0,
    "dead_tuple_threshold": 10000
  },
  "backup": {
    "enabled": false,
    "path": "./backups",
    "retention_days": 30,
    "max_backups": 10,
    "default_format": "custom",
    "default_compression": 9,
    "timeout_minutes": 30,
    "scheduler": {
      "enabled": false,
      "schedule": "0 3 * * *",
      "timezone": ""
    },
    "s3": {
      "enabled": false,
      "bucket": "",
      "region": "",
      "endpoint": "",
      "access_key_id": "",
      "secret_access_key": "",
      "path_prefix": "",
      "force_path_style": false
    }
  },
  "log": {
    "level": "info",
    "format": "text"
  }
}
